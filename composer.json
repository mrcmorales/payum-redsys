{
    "name": "mrcmorales/payum-redsys",
    "type": "library",
    "description": "Redsys gateway",
    "keywords": [
        "payment",
        "payum",
        "redsys",
        "sermepa"
    ],
    "homepage": "https://payum.forma-pro.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Morales Valldep√©rez",
            "email": "marcmorales83@gmail.com",
            "homepage": "https://github.com/mrcmorales"
        },
        {
            "name":     "Payum project",
            "homepage": "https://payum.forma-pro.com/"
        },
        {
            "name":     "Community contributions",
            "homepage": "https://github.com/Payum/AuthorizeNetAim/contributors"
        }
    ],
    "require": {
        "ext-openssl": "*",
        "ext-json": "*",
        "php": "^8.1",
        "payum/core": "^1.7",
        "php-http/message-factory": "^1.1"
    },
    "require-dev": {
        "php-http/guzzle6-adapter": "^1.0",
        "phpunit/phpunit": "^10"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "MrcMorales\\Payum\\Redsys\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MrcMorales\\Payum\\Redsys\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-1.7": "1.7-dev"
        }
    }
